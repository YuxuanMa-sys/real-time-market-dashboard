# Environment variables for the Real-Time Market Dashboard
# Copy this file to .env and fill in your actual values

# PostgreSQL connection string (Railway format)
PG_DSN=postgresql://username:password@host:port/database

# FRED API key (get from https://fred.stlouisfed.org/docs/api/api_key.html)
FRED_API_KEY=your_fred_api_key_here

# Optional: News API key if using a paid news service
# NEWS_API_KEY=your_news_api_key_here
